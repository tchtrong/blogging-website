{
  "tasks": {
    "start": "deno run --node-modules-dir -A --watch=static/,routes/ dev.ts",
    "esm:add": "deno run -A https://esm.sh/v103 add",
    "esm:update": "deno run -A https://esm.sh/v103 update",
    "esm:remove": "deno run -A https://esm.sh/v103 remove"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
